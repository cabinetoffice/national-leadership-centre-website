<div class="breadcrumbs-block">
	<div class="container">
		<div class="govuk-breadcrumbs">
			<ol class="govuk-breadcrumbs__list">
				<li class="govuk-breadcrumbs__list-item"><a class="govuk-breadcrumbs__link" href="{{ '/' | url }}">Home</a></li>
				{% if ( breadcrumbs_parent ) %}
				<li class="govuk-breadcrumbs__list-item"><a class="govuk-breadcrumbs__link" href="{{ breadcrumbs_parent_url }}">{{ breadcrumbs_parent }}</a></li>
				{% endif %}
				<li class="govuk-breadcrumbs__list-item"><a class="govuk-breadcrumbs__link" href="{{ permalink }}">{{ navtitle }}</a></li>
			</ol>
		</div>
	</div>
</div>